{
  "plots": [
    {
      "x": "container_cnt",
      "x_lbl": "#Containers",
      "y": "average",
      "y_lbl": "Average",
      "hue_lbl": "Executer",
      "hue": "execution_unit",
      "shape": "lineplot",
      "title": "Average vs. #container"
    }
  ],
  "benchmark_config": {
    "duration": 3,
    "monitors": {
      "mpstat": [
        "-n",
        "-u",
        "-I",
        "SCPU,SUM",
        "-P",
        "0,1"
      ],
      "perf": [
        "-C",
        "0,1"
      ]
    }
  },
  "cpu_order": {
    "value": "desc",
    "type": "str"
  },
  "threads": {
    "values": [
      [
        1
      ]
    ]
  },
  "containers": {
    "container_list": {
      "values": [
        {
          "min": 1,
          "step": 1,
          "max": 3
        }
      ]
    },
    "core_count": 3
  },
  "applications": [
    {
      "path": "bm-external/will-it-scale",
      "name": "malloc1_processes",
      "args": "-n -t {threads} -s {duration}",
      "adapter": {
        "name": "will-it-scale-adapter.sh"
      }
    }
  ]
}
