{
  "plots": [
    {
      "x": "container_cnt",
      "x_lbl": "#Containers",
      "y": "throughput_lps",
      "y_lbl": "Throughput(lps)",
      "hue_lbl": "Executer",
      "hue": "execution_unit",
      "shape": "barplot",
      "title": "Throughput (lps) vs. #container"
    },
    {
      "x": "container_cnt",
      "x_lbl": "#Containers",
      "y": "throughput_lps",
      "y_lbl": "Throughput",
      "hue": "execution_type",
      "hue_lbl": "Execution Env",
      "title": "Linearity vs. #Containers",
      "type": "linearity"
    }
  ],
  "benchmark_config": {
    "threads": {
      "values": [
        [
          1
        ]
      ]
    }
  },
  "containers": {
    "container_list": {
      "values": [
        [
          1,
          2,
          4,
          6,
          8
        ]
      ]
    },
    "core_count": 2
  },
  "applications": [
    {
      "path": "bm-external/byte-unixbench/UnixBench",
      "name": "Run",
      "cd": true,
      "args": "-c {threads} -i 1 -b -q syscall",
      "adapter": {
        "name": "byte-unixbench-adapter.sh"
      }
    },
    {
      "path": "bm-external/byte-unixbench/UnixBench",
      "name": "Run",
      "cd": true,
      "args": "-c {threads} -i 1 -b -q pipe",
      "adapter": {
        "name": "byte-unixbench-adapter.sh"
      }
    }
  ]
}
