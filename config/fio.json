{
  "plots": [
    {
      "x": "container_cnt",
      "x_lbl": "#Containers",
      "y": "read_MiB_bw",
      "y_lbl": "Read BW(MiB)",
      "hue_lbl": "Executer",
      "hue": "execution_unit",
      "shape": "barplot",
      "title": "BW vs. #container"
    }
  ],
  "benchmark_config": {
    "initial_size": [
      1
    ],
    "exec_env": [
      "container"
    ],
    "monitors": {
      "mpstat": [
        "-n",
        "-u",
        "-I",
        "SCPU,SUM",
        "-P",
        "0,1"
      ],
      "perf": [
        "-C",
        "0,1"
      ]
    },
    "threads": {
      "values": [
        [
          1,
          3
        ]
      ]
    }
  },
  "containers": {
    "container_list": {
      "values": [
        {
          "min": 1,
          "step": 1,
          "max": 3
        }
      ]
    },
    "core_count": 3,
    "image": "ubuntu:latest"
  },
  "applications": [
    {
      "name": "fio",
      "args": "--name=randrw --filename=/tmp/fio.test --rw=randrw --rwmixread=70 --bs=4k --size={initial_size}G --iodepth=64 --numjobs={threads} --direct=1",
      "adapter": {
        "name": "fio-adapter.sh"
      }
    }
  ]
}
